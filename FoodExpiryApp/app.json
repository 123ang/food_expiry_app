{
  "expo": {
    "name": "Expiry Alert",
    "slug": "expiry-alert-app",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/food_expiry_logo_adaptive.png",
    "userInterfaceStyle": "automatic",
    "scheme": "expiryalert",
    "sdkVersion": "53.0.0",
    "description": "Never let food go to waste again! Track expiration dates, organize your pantry, and reduce food waste with our intuitive food management app.",
    "privacy": "public",
    "splash": {
      "image": "./assets/food_expiry_logo_adaptive.png",
      "resizeMode": "contain",
      "backgroundColor": "#006b29"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expiryalert.app",
      "buildNumber": "2",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes for adding food items quickly.",
        "NSPhotoLibraryUsageDescription": "This app accesses photo library to save food item images for better organization.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/food_expiry_logo_adaptive.png",
        "backgroundColor": "#006b29"
      },
      "splash": {
        "image": "./assets/food_expiry_logo_adaptive.png",
        "resizeMode": "contain",
        "backgroundColor": "#006b29"
      },
      "package": "com.expiryalert.app",
      "versionCode": 2,
      "permissions": [
        "CAMERA",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE",
        "POST_NOTIFICATIONS"
      ]
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#006b29",
          "image": "./assets/food_expiry_logo_adaptive.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "newArchEnabled": false,
            "flipper": false
          }
        }
      ],
      "expo-sqlite"
    ],
    "extra": {
      "eas": {
        "projectId": "cb2a08f8-33c5-4312-8a50-44c6e71f0778"
      }
    },
    "owner": "123ang"
  }
}
