{
  "expo": {
    "name": "Expiry Alert",
    "slug": "expiry-alert-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/food_expiry_logo.png",
    "userInterfaceStyle": "automatic",
    "scheme": "expiryalert",
    "sdkVersion": "53.0.0",
    "splash": {
      "image": "./assets/food_expiry_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#228B22"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expiryalert.app",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash-icon.png",
        "backgroundColor": "#4CAF50"
      },
      "icon": "./assets/food_expiry_logo.png",
      "roundIconImage": "./assets/food_expiry_logo.png",
      "package": "com.expiryalert.app",
      "versionCode": 5,
      "permissions": [
        "CAMERA",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "WRITE_EXTERNAL_STORAGE",
        "READ_EXTERNAL_STORAGE"
      ]
    },
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-notifications",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "cb2a08f8-33c5-4312-8a50-44c6e71f0778"
      }
    },
    "owner": "123ang"
  }
}
